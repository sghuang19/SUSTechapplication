---
import Chip from "./Chip.astro";
const { data } = Astro.props;
---

<div class="flex flex-wrap gap-2">
  <Chip text={`${data.year} çº§`} variant="primary" icon="calendar" />
  <Chip text={data.region.toUpperCase()} variant="accent" icon="map-pin" />
  <Chip text={data.university} variant="success" icon="academic-cap" />
  <Chip text={data.major} variant="primary" icon="book-open" />
  <Chip text={data.program} variant="primary" icon="trophy" />
</div>
